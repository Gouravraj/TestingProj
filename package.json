{
  "name": "distribution-mobile-automation",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:ios": "jest ./tests/ios-login-page.test.js --detectOpenHandles",
    "test:android": "jest ./tests/android-login-page.test.js --detectOpenHandles",
    "gen-conf": "node ./scripts/pkg/generate-config.js",
    "pkg:ios": "npm run gen-conf && extract-pkg ios --config=./scripts/pkg/config.json",
    "pkg:android": "npm run gen-conf && extract-pkg android --config=./scripts/pkg/config.json",
    "appium": "appium",
    "appium:doctor": "appium-doctor"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "0.59.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "appium": "^1.12.1",
    "appium-doctor": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-preset-react-native": "^4.0.1",
    "colors": "^1.3.3",
    "eslint": "^5.16.0",
    "extract-pkg": "^1.1.3",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.54.0",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "react-test-renderer": "16.8.3",
    "wd": "^1.11.1",
    "yargs": "^13.2.2"
  }
}
